{
  "name": "fitme",
  "version": "1.1.9",
  "description": "Workout management web app",
  "main": "index.js",
  "scripts": {
    "meteor": "meteor --port $IP:$PORT --settings settings-development.json",
    "deploy-production": "DEPLOY_HOSTNAME=galaxy.meteor.com meteor deploy --settings settings.json www.gofitme.com",
    "eslint-fix": "eslint . --fix"
  },
  "eslintConfig": {
    "plugins": [
      "react"
    ],
    "extends": [
      "airbnb",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "quotes": [
        "error",
        "single"
      ],
      "spaced-comment": [
        "error",
        "always"
      ],
      "import/no-unresolved": [
        2,
        {
          "ignore": [
            "^meteor/"
          ]
        }
      ]
    },
    "globals": {
      "_": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mistaken3lf/FitMe.git"
  },
  "keywords": [
    "Workout",
    "Fitness",
    "FitMe",
    "Exercise",
    "Trainers"
  ],
  "author": "Mistaken3lf, Madams92, Mcphersonad21",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mistaken3lf/FitMe/issues"
  },
  "homepage": "https://github.com/Mistaken3lf/FitMe#readme",
  "dependencies": {
    "bootstrap": "^3.3.6",
    "jquery": "^2.2.4",
    "moment": "^2.13.0",
    "react": "^15.1.0",
    "react-addons-pure-render-mixin": "^15.1.0",
    "react-bootstrap": "^0.29.4",
    "react-dom": "^15.1.0",
    "react-komposer": "^1.8.0",
    "react-mounter": "^1.2.0",
    "react-router": "^2.4.1",
    "react-s-alert": "^1.1.4",
    "sweetalert": "^1.1.3"
  },
  "devDependencies": {
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.0",
    "eslint-plugin-jsx-a11y": "^1.4.2",
    "eslint-plugin-react": "^5.1.1"
  }
}
